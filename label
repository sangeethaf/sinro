import matplotlib.pyplot as plt
import numpy as np
"""(1,2),(2,3),(5,6),(3,1)"""
xpoints=np.array([1,2,3,5])
ypoints=np.array([2,3,1,6])
#plt.plot(xpoints,ypoints,'*:k')
plt.plot(xpoints,ypoints,marker='*',ms=18,markerfacecolor='r')
plt.title("sample")
plt.xlabel("x axis")
plt.ylabel("y axis")
plt.grid(axis='x')
plt.show()
